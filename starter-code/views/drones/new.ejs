<h2>New Drone</h2>
<form action="/drones" method="POST">
  <% if (typeof(errors) !== "undefined"){ %>
    <% for (let error in errors){ %>
      <li><%= errors[error].message %></li>
    <% } %>
  <% } %>
  <div class="form-group">
    <label for="droneName">Name:</label>
    <input type="text" name="droneName" id="dronaName" class="form-control">
  </div>
  <div class="form-group">
    <label for="propellers">Propellers:</label>
    <input type="number" name="propellers" id="propellers" class="form-control">
  </div>
  <div class="form-group">
    <label for="maxSpeed">Max Speed:</label>
    <input type="number" name="maxSpeed" id="maxSpeed" class="form-control">
  </div>
  <button type="submit" class="btn btn-primary">Create</button>
</form>
